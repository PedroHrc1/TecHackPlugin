{
    "manifest_version": 2,
    "name": "Extensão de Segurança Web",
    "version": "1.5",
    "description": "Detecta conexões a domínios de terceiros, ameaças de hijacking, uso de armazenamento local, Canvas Fingerprinting e cookies.",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "<all_urls>",
      "tabs",
      "storage",
      "cookies"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_title": "Segurança Web",
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"],
        "run_at": "document_start"
      }
    ]
  }
  